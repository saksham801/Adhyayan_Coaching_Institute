{
  "version": 2,
  "builds": [
    { "src": "Adhyayan_Coaching_Institute/wsgi.py", "use": "@vercel/python", "config": { "runtime": "python3.9" } },
    { "src": "build_files.sh", "use": "@vercel/static-build", "config": { "distDir": "staticfiles" } }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "<projectname>/wsgi.py" }
  ]
}
